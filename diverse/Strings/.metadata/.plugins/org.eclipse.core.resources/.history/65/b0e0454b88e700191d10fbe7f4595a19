
public class Class1 {

	public static void main(String[] args) {
		String s1 = "si";
		String s2 = "sir1 si cu sir2 fac un sir3";
		int k = 0;
		for (int i=0; i < s2.length(); i++) {
			for(int j=i; j < s2.length(); j++)
			String s3 = s2.substring(i,j);
			if(s3.equals(s1))
		       k++;
		}
		
		System.out.println(k);

	}

}
