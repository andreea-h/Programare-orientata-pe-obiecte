
public class NrAparitii {
	public static int Aparitii(String s1, String s2) {
		System.out.println(s2.split(" ").length());
		return 1;
	}
	
	public static void main(String[] args) {
		String s1 = args[0];
		String s2 = args[1];
		//Aparitii obiect = new Aparitii();
		System.out.println(Aparitii(s1,s2));
	}

}
