	public static String cenzurare(String text, String cuvinte[]) {
	//	StringTokenizer sir = new StringTokenizer(text," ");
		int i, pos;
		for (i=0; i<cuvinte.length; i++) {
			pos = text.indexOf(cuvinte[i],0);
			}
		}
	}
