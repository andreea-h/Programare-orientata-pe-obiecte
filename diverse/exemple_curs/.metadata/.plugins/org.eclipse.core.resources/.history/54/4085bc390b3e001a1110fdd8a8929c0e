
public class Animal {
	
	public String type = "mamifer";
	public void show() {
		System.out.println("Animalul este un: " + type);
	}
}

public class Dog extends Animal {
	public String type;
	public Dog(String type) {
		this.type = type;
	}
	public void show() {
		System.out.println("Cainele este un: " + type); 
	}
}
