
public class Problema2 {
	
	//folosind metoda split
	static int aparitii(String s1, String s2) {
		String s3[] = s1.split(" ");
		int i;
		int k=0;
		for(i=0;i<s3.length;i++) {
			if(s3[i].equals(s2)) {
				k++;
			}
		}
		return k;
	}
	
	//folosind metoda StringTokenizer
	static int(String s1, String s2) {
		int k=0;
		StringTokenizer s = new StringTokenizer(s1);
		
	}
	
	public static void main(String[] args) {
		String s1=new String(args[0]);
		String s2=new String(args[1]);
		System.out.println(aparitii(s1,s2));
		
	}

}
