
class Train {
	Route r;
	Schedule s;
	boolean local;
	
}

class Schedule {
	ClockTime departure;
	ClockTime arrival;
}

class Route {
	String origin;
	String destination;
}

class ClockTime {
	int hour;
	int minute;
	
	public ClockTime(int hour, int minute) {
		this.hour = hour;
		this.minute = minute;
	}
	
}
