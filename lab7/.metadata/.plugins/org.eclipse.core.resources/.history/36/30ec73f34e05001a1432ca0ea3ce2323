import java.util.*;
import java.io.*;
public class Problema5 {
	HashMap<String, Integer> a1;
	TreeMap<String, Integer> a2;
	
	public Problema5() {
		a1=new HashMap();
		a2=new TreeMap();
	}
	public HashMap creeazaMap(String director) {
		File f = new File(director);
		String[] file = f.list();
		int i;
		for(i=0;i<file.length;i++)
			a1.put(file[i],(file[i].length()));
		return a1;
	}
	
	public afisare() {
		
	}

}
