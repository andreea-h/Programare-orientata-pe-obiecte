
abstract class Hero extends BadLuck {
	
}

abstract class Warrior extends Hero { 
	
}

abstract class Ninja extends Warrior {
  
}

abstract class Rogue extends Ninja {
	
}

abstract class StormFire {
	/*void execute(StormFire r) {
