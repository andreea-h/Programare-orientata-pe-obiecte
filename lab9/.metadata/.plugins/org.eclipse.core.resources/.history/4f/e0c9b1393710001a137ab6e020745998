class StormFire extends BadLuck {
	 void execute(Hero h) {
		 System.out.println("in StormFire class/for Hero");
	 }
    void execute(Warrior w) {
    	 System.out.println("in StormFire class/for Warrior");
    }
    void execute(Ninja n) {
    	 System.out.println("in StormFire class/for Ninja");
    }
    void execute(Rogue r) {
    	 System.out.println("in StormFire classfor Rogue");
    }
}